{% extends "DevBieresTaskWebHtmlBundle::layout.html.twig" %}

{% block content_title %}
   <div class="content_title_action" >
        <a href="{{ path('web_projet_index') }}" >
            {{ 'site.back'|trans }}
            <img src="{{ asset('bundles/devbierestaskwebhtml/images/back.png') }}" alt="{{ 'site.back'|trans }}" />
        </a>
   </div>
   {{ 'site.destroy'|trans }} [{{ obj.libelle }}]
{% endblock %}

{% block content %}
   <div id="destroyprojet" class="formulaire"  >

     <form action="{{ path('web_projet_destroy_confirmed') }}" method="post" {{ form_enctype(form) }}>
       <ul>
         <li class="error" >
             {{ form_errors(form) }}
         </li>
         <li class="error" >
             {{ form_errors(form.id) }}
         </li>
         <li>
             {{ 'site.confirm'|trans }}
         </li>
         <li>
              <label></label>
              <input type="submit" value="{{ 'site.valid'|trans }}" id="submit" />
         </li>

         {{ form_rest(form) }}
        </ul>
      </form>

   </div>
{% endblock %}
